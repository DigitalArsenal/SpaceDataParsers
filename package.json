{
  "name": "spacedataparsers",
  "version": "1.0.0",
  "description": "Parsers for Space Data Messages",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon  --harmony-top-level-await --trace-deprecation --experimental-wasm-modules test/test.mjs -w test/test.mjs -w  test/omm.test.mjs -w  test/satcat.test.mjs -w src/index.mjs",
    "update:schema": "./node_modules/.bin/ajv migrate -s ./test/_root_IDLDocument.schema.json",
    "build:sgp4:wasm": "cd ./src/SGP4Propagator && npm run build && sed -i -E 's/__dirname//' ./wasmmodule.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalArsenal/SpaceDataParsers.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DigitalArsenal/SpaceDataParsers/issues"
  },
  "homepage": "https://github.com/DigitalArsenal/SpaceDataParsers#readme",
  "dependencies": {
    "ajv": "^6.12.4",
    "bignumber.js": "^9.0.0",
    "btoa": "^1.2.1",
    "neat-csv": "^5.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "ajv-cli": "^3.2.1",
    "nodemon": "^2.0.4",
    "tape": "^5.0.1"
  },
  "type": "module"
}
